{
  "name": "observe-on-scope",
  "version": "0.1.0",
  "description": "RxJS operator for using Observables with angularjs (1.x)",
  "main": "dist/cjs/index.js",
  "jsnext:main": "dist/es6/index.js",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "npm-run-all -p build:*",
    "build:es6": "tsc -p src/ -m es6 -t es6 --outDir ./dist/es6",
    "build:cjs": "tsc -p src/ -m commonjs -t es5 --outDir ./dist/cjs",
    "test": "mocha --opts mocha.opts"
  },
  "keywords": [
    "rxjs",
    "angular1",
    "angularjs"
  ],
  "author": "Wolfgang Goedel",
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "^1.5.16",
    "@types/angular-mocks": "^1.5.5",
    "@types/chai": "^3.4.34",
    "@types/isomorphic-fetch": "0.0.31",
    "@types/jsdom": "^2.0.29",
    "@types/mocha": "^2.2.32",
    "@types/sinon": "^1.16.31",
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "chai": "^3.5.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^9.6.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.1.0",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0-rc.1",
    "sinon": "^1.17.6",
    "ts-node": "^1.4.1",
    "typescript": "^2.0.3"
  },
  "peerDependencies": {
    "angular": "^1.5.8",
    "rxjs": "^5.0.0-rc.1"
  }
}
